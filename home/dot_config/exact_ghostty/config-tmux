# vim: syntax=ghostty

# keybinds for tmux
# https://www.eso.org/~ndelmott/ascii.html
# ctrl+b is the prefix for tmux
# \x02 is ctrl+b

# ctrl+b then c
keybind=super+n=text:\x02c

# ctrl+b then c
keybind=super+t=text:\x02c

# ctrl+b then q
keybind=super+w=text:\x02q

# ctrl+b then \
keybind=super+d=text:\x02\x5c

# ctrl+b then -
keybind=super+shift+d=text:\x02\x2d
